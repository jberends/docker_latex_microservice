### Multipart form with multiple media files.
POST {{host}}/api/pdf
Content-Type: multipart/form-data; boundary=boundary

--boundary
Content-Disposition: form-data; name="tex"
Content-Type: text/plain

Some Information that should be a latex file </> \\ üëçüèº  " '
--boundary
Content-Disposition: form-data; name="media"; filename="some.pdf"
Content-Type: application/pdf

< ../assets/lorum_ipsum.pdf
--boundary
Content-Disposition: form-data; name="media"; filename="another.pdf"
Content-Type: application/pdf

< ../assets/lorum_ipsum.pdf
--boundary

> {%
client.test("success", function() {
  client.assert(response.status === 200, "Response status is not 200");
});
%}

